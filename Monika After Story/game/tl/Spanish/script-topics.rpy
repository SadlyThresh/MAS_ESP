    # m 1eua "Hey [mas_get_player_nickname(regex_replace_with_nullstr='my ')], have you ever had a pet?"
    m 1eua "Hey, [mas_get_player_nickname(regex_replace_with_nullstr='my')], ¿alguna vez has tenido una mascota?"

