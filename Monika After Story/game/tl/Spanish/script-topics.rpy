translate Spanish monika_player_read_poetry_1d430bd7:

    # m 1eua "You know that I like poetry, but I was wondering..."
    m 1eua "Sabes que me gusta la poesía, pero me preguntaba..."

# game/script-topics.rpy:13002
translate Spanish monika_player_read_poetry_f107a71b:

    # m 3eua "Do you often read poetry?{nw}"
    m 3eua "¿Lees poesía a menudo?{nw}"

# game/script-topics.rpy:13004
translate Spanish monika_player_read_poetry_fee746bb:

